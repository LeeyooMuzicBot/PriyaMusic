{
    "name": "Priya Music Bot",
    "description": "Telegram bot for Streaming  Music trought the Telegram Group Video Chat, powered by pytgcalls and pyrogram",
    "logo": "https://telegra.ph//file/a855ceaa3910d17a3174a.jpg",
    "keywords": [
        "pytgcalls",
        "telegram bot",
        "pyrogram"
    ],
    "website": "https://t.me/PriyaUpdates",
    "repository": "https://github.com/MyLoveMoon/PriyaMusicBot",
    "success_url": "https://t.me/PriyaUpdates",
    "env": {
        "API_ID": {
            "description": "your API_ID from my.telegram.org",
            "required": true
        },
        "API_HASH": {
            "description": "your API_HASH from my.telegram.org",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "your bot token from @BotFather",
            "required": true
        },
        "BOT_USERNAME": {
            "description": "your bot username from @BotFather",
            "required": false
            "value": "PriyaMuzicBot"
        },
        "BOT_NAME": {
            "description": "fill with your bot name from @BotFather",
            "required": false
        },
        "ASSISTANT_NAME": {
            "description": "fill with the assistant username account without @",
            "required": false
            "value": "shhdgdd"
        },
        "SESSION_NAME": {
            "description": "A Pyrogram String Session",
            "required": true
        },
        "SUDO_USERS": {
            "description": "list of user ids to be added to sudo member list, or just fill with your id",
            "required": true,
            "value": "5413065936"
        },
        "GROUP_SUPPORT": {
            "description": "if you have group, then fill the group username here without @",
            "required": false,
            "value": "PriyaUpdates"
        },
        "UPDATES_CHANNEL": {
            "description": "if you have channel, then fill the channel username here without @",
            "required": false,
            "value": "PriyaUpdates"
        },
        "OWNER_NAME": {
            "description": "fill with your telegram account username without @",
            "required": false,
            "value": "shhdgdd"
        },
        "ALIVE_NAME": {
            "description": "fill with your telegram account nickname/name",
            "required": false,
            "value": "ᴘʀɪʏᴀ ᴍᴜsɪᴄ"
        }
    },
    "addons": [],
    "buildpacks": [
        {
            "url": "heroku/python"
        },
        {
            "url": "heroku/nodejs"
        },
        {
            "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "eco"
        }
    },
    "stack": "container"
    }
